/* pages/youh_xq/youh_xq.wxss */

.preferential_details {
    padding: 0 30rpx;
    margin-top: 40rpx;
}

.prefer_logo {
    height: 390rpx;
    border-radius: 10rpx;
    overflow: hidden;
    box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
}

.prefer_logo image {
    width: 100%;
    height: 390rpx;
}

.prefer_infor {
    box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
    background: #ffffff;
    margin-top: 30rpx;
    padding: 40rpx;
}

.p_in_t {
    margin-bottom: 40rpx;
}

.p_in_t:last-child {
    margin-bottom: 0;
}

.p_in_t image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 30rpx;
}

.p_in_t text {
    font-size: 24rpx;
    line-height: 34rpx;
}

.p_in_t .cl {
    background: #f5f5f9;
    width: 140rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    font-size: 24rpx;
    margin-left: 30rpx;
    border-radius: 50rpx;
}